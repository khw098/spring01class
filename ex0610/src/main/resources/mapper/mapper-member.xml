<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<!-- mvc_board 모든 게시물을 전달 -->  
<mapper namespace="com.site.mapper.MemberMapper">

	<!-- login userid,upw 검색 -->
	<select id="selectLogin" resultType="com.site.vo.Mvc_MemberVo">
	    select * from mvc_member where userid=#{userid} and upw=#{upw} 
	</select>
	
	
	
</mapper>